// A simple, imperative style, fibonacci function written in Lintilla.
//

fn fibonacci(n : int) -> int {
  if n < 0 { return -1 } else {}; // If `n` is negative return the error value -1.
  
  let mut res1 = 0;
  let mut res2 = 1;
  let mut count = n;
  
  while 0 < count {
    let temp = res1;
    res1 := res2;
    res2 := temp + res1;
    count := count - 1
  };
  
  res1
};

fibonacci(5)